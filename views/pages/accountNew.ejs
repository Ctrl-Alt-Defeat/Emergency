<body class="is-preload">

	<!-- Header -->
	<div id="header">

		<div class="top">
			<% let visible=is_not_enable ?'none': 'block' %>
			<% let visibleCont = is_not_enable ? 'block':'none'%>

					<!-- Logo -->
					<div id="logo">
						<span class="image avatar48"><img src="<%= data.img %>" alt="" /></span>
						<h1 id="title">
							<%= data.username %>
						</h1>
						<p>
							<%= data.type_of_work%>
						</p>
					</div>

					<!-- Nav -->
					<nav id="nav">
						<ul>
							<li><a href="#top" id="top-link"><span class="far fa-address-card">About me</span></a></li>
							<li><a href="#portfolio" id="portfolio-link"><span
										class="far fa-comment">Feedbacks</span></a></li>
							<li><a href="#Edit" style="display: <%= visible%>;" id="portfolio-link"><span
										class="far fa-edit">Edit Profile</span></a></li>
							<li><a href="#about" id="about-link"><span class="far fa-calendar-alt">Schedule</span></a>
							</li>
							<li><a href="#contact" id="contact-link"><span class="icon fa-envelope">Contact</span></a>
							</li>
						</ul>
					</nav>

		</div>

		<div class="bottom">

			<!-- Social Icons -->
			<ul class="icons">
				<li><a href="/" class="icon fa-twitter"><span class="label">Twitter</span></a></li>
				<li><a href="/" class="icon fa-facebook"><span class="label">Facebook</span></a></li>
				<li><a href="/" class="icon fa-github"><span class="label">Github</span></a></li>
				<li><a href="/" class="icon fa-linkedin"><span class="label">LinkedIn</span></a></li>
				<li><a href="<%= data.email%>" class="icon fa-envelope"><span class="label">Email</span></a></li>
			</ul>

		</div>

	</div>

	<!-- Main -->
	<div id="main">

		<!-- Intro -->
		<section id="top" class="one dark cover" style="padding: 2em 0;">
			<div class="container">

				<header>
					<h2 class="alt">Hello <strong>
							<%= data.full_name%>
						</strong>, welcome in <a href="/">Emergency </a>-App.<br />

				</header>
			</div>
		</section>

		<!-- About Me -->
		<section id="about" class="three">
			<div class="container">

				<header style="margin: 0 0 1em 0;">
					<h2>About <%= data.username %>
					</h2>
				</header>

				<footer>
					<!-- <a href="#portfolio" class="button scrolly">Portfolio</a> -->
				</footer>
				<a href="#" id="funnyimg" class="image featured"><img src="images/funny3.gif" alt="" /></a>
				<div class="div1">
					<div class="div2">
						<div class="border">
							Your name : <%= data.full_name%>
						</div>
						<div class="border">
							Major : <%= data.type_of_work%>
						</div>
						<div class="border">
							experiance : <%= data.exp%>
						</div>

					</div>
					<div class="div3">

						<div class="border">
							location : <%= data.location%>
						</div>
						<div class="border">
							Role : <%= data.role%>
						</div>
						<div class="border">
							status : <%= data.status%>
						</div>



					</div>
				</div>

			</div>
